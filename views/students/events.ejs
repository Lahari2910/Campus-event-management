
<h2>All Events</h2>
<a href="/student/my-events">My Registered Events</a> | 
<a href="/student/logout">Logout</a>
<table border="1" cellpadding="10">
  <tr>
    <th>Title</th><th>Date</th><th>Time</th><th>Venue</th><th>Action</th>
  </tr>
  <% events.forEach(event => { %>
    <tr>
      <td><%= event.title %></td>
      <td><%= event.date.toISOString().slice(0,10) %></td>
      <td><%= event.time %></td>
      <td><%= event.venue %></td>
      <td>
        <form action="/student/events/register/<%= event.id %>" method="POST">
          <button type="submit">Register</button>
        </form>
      </td>
    </tr>
  <% }) %>
</table>
